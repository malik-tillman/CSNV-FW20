<div class="fw20__container">
    <div class="fw20-image__container">
        <video autoplay muted loop>
            <source src="https://cdn.shopify.com/s/files/1/0188/6325/9699/files/hero-video.mp4?v=1599159038">
        </video>
        <div class="fw20-image-overlay">
            <img src="{{ section.settings.image | img_url:'master' }}" alt="{{ section.settings.imageAlt }}">
        </div>
    </div>
</div>

{% schema %}
{
"name": "FW20",
"settings":[
{
"type": "header",
"content": "Fall/Winter 2020"
},
{
"type":"image_picker",
"id":"image",
"label": "Image"
},
{
"type": "text",
"id": "imageAlt",
"label": "Image's alt text"
}
],
"presets":[
{
"name": "FW20",
"category": "Image",
"settings":{}
}
]
}
{% endschema %}
